<% content_for(:title, "Home | @ThePark!") %>
<div class='container'>
  <div class="row">
    <div class='col-md-3 col-md-offset-3'>
      <h2>Parks</h2>
      <ul>
        <% @parks.all.each do |park| %>
          <li><%= link_to "#{park.name}", "/parks/#{park.id}" %></li>
        <% end %>
      </ul>
    </div>
    <div class='col-md-3'>
      <h2>Users</h2>
      <ul>
        <% @users.all.each do |user| %>
          <li><%= link_to "#{user.name}", "/users/#{user.id}" %></li>
        <% end %>
      </ul>
    </div>
  </div>
</div>
